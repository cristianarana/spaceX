{
    "version": 2,
    "builds": [
      {
        "src": "src/app.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/launches",
        "dest": "/dist/app.js"
      },
      {
        "src": "/api/rockets",
        "dest": "/dist/app.js"
      },
      {
        "src": "/api/flights",
        "dest": "/dist/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/app.js"
      }
    ]
  }